@model Dictionary<int, int>
@{
    ViewBag.Title = "AllNumbersStatisticsResults";
}

<h2>AllNumbersStatisticsResults</h2>

<table>
    <thead>
        <tr><th>Liczba</th><th>Ilosc</th></tr>
    </thead>
    <tbody>
        @for(int i = 0; i < 50; ++i)
        {
            <tr>
                <td>@(i + 1)</td>
                <td>@Model[i]</td>
            </tr>
        }       
    </tbody>
</table>