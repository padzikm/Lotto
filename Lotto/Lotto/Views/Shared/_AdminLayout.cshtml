<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width" />
    <title>@ViewBag.Title</title>
    @Styles.Render("~/Content/bootstrap.css")
    @Styles.Render("~/Content/themes/base/jquery.ui.all.css")     
    @Scripts.Render("~/Scripts/modernizr-2.5.3.js")    
    @Scripts.Render("~/Scripts/jquery-1.9.1.js")            
    @Scripts.Render("~/Scripts/jquery-ui-1.8.20.min.js")
    @Scripts.Render("~/Scripts/jquery.validate.js")
    @Scripts.Render("~/Scripts/jquery.validate.unobtrusive.js")         
    <script src="http://code.jquery.com/jquery-migrate-1.0.0.js" type="text/javascript"></script>
    <script>
        $(function () {
            $(".datepicker").datepicker();
        });
</script>
</head>
<body>
    <div class="container-fluid">
    <div class="row">
        <div class="col-md-12">
            @if(ViewBag.Success != null)
            {
                <div class="alert alert-success">@ViewBag.Success</div>
            }
            @{Html.RenderPartial("Header");}            
        </div>
    </div>
    <div class="row">
        <div class="col-md-4">
            @{Html.RenderAction("AdminMenu", "Navigation");}                        
        </div>
        <div class="col-md-8">
            @RenderBody()
        </div>
    </div>
</div>        
    @RenderSection("scripts", required: false)
</body>
</html>
